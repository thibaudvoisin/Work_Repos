{
  "name": "front-end",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ext .js,.svelte src",
    "lint:fix": "eslint --fix --quiet src --ext .js,.svelte",
    "format": "prettier --check  --plugin prettier-plugin-svelte .",
    "format:fix": "prettier --write --log-level=warn  --plugin prettier-plugin-svelte ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/parser": "^6.6.0",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-svelte": "^2.33.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.4.3",
    "svelte-eslint-parser": "^0.33.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "fuse.js": "^6.6.2",
    "theme-change": "^2.5.0"
  }
}
